{
  "functions": {
    "api/analyze-parts.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/feedback-chat.js": {
      "maxDuration": 30,
      "memory": 512
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    }
  ]
}